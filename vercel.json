{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "captura.html",
        "use": "@vercel/static"
      },
      {
        "src": "obrigado.html",
        "use": "@vercel/static"
      },
      {
        "src": "vendas-abertas.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/captura",
        "dest": "/captura.html"
      },
      {
        "src": "/obrigado",
        "dest": "/obrigado.html"
      },
      {
        "src": "/vendas-abertas",
        "dest": "/vendas-abertas.html"
      }
    ]
  }
  